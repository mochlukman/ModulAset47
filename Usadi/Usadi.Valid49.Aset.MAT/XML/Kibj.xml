  <statements>
    <insert id="InsertKibj" parameterMap="insert-param">
      <generate table="Aset_Kibjdet"/>
    </insert>
    <update id="UpdateByKibj" parameterClass="Kibj">
      update ASET_KIBSPESIFIKASI set KDPEMILIK=#Kdpemilik#, ASALUSUL=#Asalusul#, PENGGUNA=#Pengguna#, KET=#Ket#, KDHAK=#Kdhak#
      , LUASTNH=#Luastnh#, ALAMAT=#Alamat#, NOFIKAT=#Nofikat#, TGFIKAT=#Tgfikat#,
      UTARA=#Utara#, TIMUR=#Timur#, SELATAN=#Selatan#, BARAT=#Barat#, LOKASI=#Lokasi#,KOORDINAT=#Koordinat#
      ,MERKTYPE=#Merktype#,KDSATUAN=#Kdsatuan#,UKURAN=#Ukuran#,BAHAN=#Bahan#,KDWARNA=#Kdwarna#,NOPABRIK=#Nopabrik#,NORANGKA=#Norangka#,NOPOLISI=#Nopolisi#,NOBPKB=#Nobpkb#,NOMESIN=#Nomesin#
      ,BERTINGKAT=#Bertingkat#,BETON=#Beton#,LUASLT=#Luaslt#,NODOKGDG=#Nodokgdg#,NOKDTANAH=#Nokdtanah#
      ,KONSTRUKSI=#Konstruksi#,PANJANG=#Panjang#,LEBAR=#Lebar#,LUAS=#Luas#,NODOKJIJ=#Nodokjij#
      ,JDLPENERBIT=#Jdlpenerbit#,BKPENCIPTA=#Bkpencipta#,SPESIFIKASI=#Spesifikasi#,ASALDAERAH=#Asaldaerah#,PENCIPTA=#Pencipta#,JENIS=#Jenis#
      where IDBRG=#Idbrg# and URUTHIST=#Uruthist#
    </update>
    <update id="UpdateByStatuspenggunaKibj" parameterClass="Kibj">
      update ASET_KIB set STATUSPENGGUNA = '0' where IDBRG=#Idbrg#
    </update>
    <update id="UpdateByValidRevKibj" parameterClass="Kibj">
      update ASET_KIBJDET set REV_BY=#Rev_by# and REV_DATE=#Rev_date#
    </update>
    <update id="UpdateByUnValidRevKibj" parameterClass="Kibj">
      update ASET_KIBJDET set REV_BY='' and REV_DATE=null
    </update>
    <delete id="DeleteByKibj" parameterMap="delete-param">
      <generate table="Aset_kib" by="IDBRG"/>
    </delete>
    <delete id="DeleteBySpesifikasiKibj" parameterClass="Kibj">
      delete from ASET_KIBSPESIFIKASI where IDBRG=#Idbrg# and URUTHIST=#Uruthist#
    </delete>
    <delete id="DeleteByKibKibj" parameterClass="Kibj">
      delete from ASET_KIB where IDBRG=#Idbrg# and UNITKEY=#Unitkey# and ASETKEY=#Asetkey#
    </delete>
    <select id="BaseQueryKibj" parameterClass="Kibj" resultMap="return-result">
    </select>
    <select id="QueryByAllKibj" parameterClass="Kibj" resultMap="return-result">
      exec REGISTER_KIBJ #Asetkey#, #Unitkey#, #Kdkib#
    </select>
    <select id="QueryByKdtanahKibj" parameterClass="Kibj" resultMap="return-result-kdtanah">
      select AA.ID, X.IDBRG, X.ASETKEY, DA.KDASET, DA.NMASET, A.TGLPEROLEHAN, AA.TAHUN, AA.NOREG, X.NILAI
      , AA.ALAMAT, AA.KET, X.IDBRG as NOKDTANAH, AA.ALAMAT AS NMKDTANAH,AA.KOORDINAT
      FROM (
      select A.IDBRG, A.UNITKEY, A.ASETKEY, SUM((CASE WHEN LEFT(A.KDTANS,1)=2 THEN -1*A.NILAI ELSE A.NILAI END)) AS NILAI
      from ASET_KIBJDET A
      where A.UNITKEY=#Unitkey# AND A.KDKIB=#Kdkib#
      group by A.IDBRG, A.UNITKEY, A.ASETKEY
      ) X
      inner join ASET_KIB A ON A.IDBRG = X.IDBRG
      inner join ASET_KIBSPESIFIKASI AA ON AA.IDBRG = X.IDBRG AND AA.UNITKEY = X.UNITKEY AND AA.ASETKEY = X.ASETKEY
      inner join DAFTASET DA ON DA.ASETKEY = X.ASETKEY
      where X.NILAI > 0
    </select>
    <select id="LoadByMinuruthistKibj" parameterClass="Kibj" resultMap="return-minuruthist">
      select min(cast(URUTHIST as int)) MINURUTHIST from ASET_KIBSPESIFIKASI where IDBRG = #Idbrg#
    </select>
    <select id="LoadByJmlkibdetKibj" parameterClass="Kibj" resultMap="return-jmlkibdet">
      select COUNT(*) JMLKIBDET from ASET_KIBJDET where IDBRG=#Idbrg# and URUTTRANS=#Uruthist#
    </select>
    <select id="LoadByJmlskpenggunaKibj" parameterClass="Kibj" resultMap="return-jmlskpengguna">
      select COUNT(*) JMLSKPENGGUNA from ASET_SKPENGGUNADETBRG A where A.IDBRG=#Idbrg#
    </select>
